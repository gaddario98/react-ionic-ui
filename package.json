{
  "name": "@gaddario98/react-ionic-ui",
  "version": "1.0.0",
  "main": "dist/index.js",
  "peerDependencies": {
    "@capacitor/core": "*",
    "@gaddario98/react-auth": "*",
    "@gaddario98/react-ionic-utiles": "*",
    "@gaddario98/react-localization": "*",
