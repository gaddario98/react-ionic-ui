    "@ionic/react": "*",
    "@mapbox/search-js-react": "*",
    "@tailwindcss/cli": "^4.1.3",
    "@tailwindcss/postcss": "^4.1.3",
    "@tanstack/react-query": "*",
    "@tanstack/react-table": "*",
    "i18next": "*",
    "mapbox-gl": "*",
    "postcss": "^8.5.3",
    "react": "*",
    "react-i18next": "*",
    "tailwindcss": "^4.1.3"
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
